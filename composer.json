{
    "name": "hanterrian/visual-composer-module",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "hanterrian",
            "email": "hanterrian@ukr.net"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "2amigos/yii2-tinymce-widget": "~1.1",
        "yiisoft/yii2-bootstrap4": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "visualComposer\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 1800,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "fxp-asset": {"enabled": false}
    }
}
